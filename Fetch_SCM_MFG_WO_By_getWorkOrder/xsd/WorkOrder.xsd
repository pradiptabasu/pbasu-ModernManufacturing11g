<?xml version= '1.0' encoding= 'UTF-8' ?>
<xsd:schema elementFormDefault="qualified" targetNamespace="http://xmlns.oracle.com/apps/scm/commonWorkExecution/workOrders/workOrdersService/"
     sdoJava:package="oracle.apps.scm.commonWorkExecution.workOrders.workOrdersService"
     xmlns:ns0="http://xmlns.oracle.com/adf/svc/types/" xmlns:sdoJava="commonj.sdo/java" xmlns:sdoXML="commonj.sdo/xml"
     xmlns="http://xmlns.oracle.com/apps/scm/commonWorkExecution/workOrders/workOrdersService/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:annotation>
        <xsd:documentation xmlns:oer="http://xmlns.oracle.com/oer">
            <name>Work Order Details</name>
            <description>An object, representing the work order details, that is returned
                for the given work order</description>
            <oer:category name="lbo">
                <value>SCM-SCM : WIE-Common Work Execution : ORA_WORK_ORDER-Work
                    Order</value>
            </oer:category>
            <oer:attributes>
                <oer:attribute description="Actual completion date and time of the Work Order" name="ActualCompletionDate"/>
                <oer:attribute description="Actual start date and time of the Work Order" name="ActualStartDate"/>
                <oer:attribute description="Flag indicating that this work order is created as a result of the back to back flow"
                     name="BackToBackFlag"/>
                <oer:attribute description="Date and time at which work order is cancelled" name="CanceledDate"/>
                <oer:attribute description="Reason to cancel a work order" name="CanceledReason"/>
                <oer:attribute description="Date and time on which a work order is closed" name="ClosedDate"/>
                <oer:attribute description="Contract manufacturing purchase order header id" name="CmPoHeaderId"/>
                <oer:attribute description="Contract manufacturing purchase order line id" name="CmPoLineId"/>
                <oer:attribute description="Contract manufacturing purchase order line location id"
                     name="CmPoLineLocId"/>
                <oer:attribute description="Unique indentifier of the completion locator" name="ComplLocatorId"/>
                <oer:attribute description="Subinventory where the work order product would be completed into inventory"
                     name="ComplSubinventoryCode"/>
                <oer:attribute description="Completed quantity" name="CompletedQuantity"/>
                <oer:attribute description="Flag indicating if the Work Order is meant for contract manufacturing"
                     name="ContractMfgFlag"/>
                <oer:attribute description="Flag indicating if the Work Order is meant for firm planned"
                     name="FirmPlannedFlag"/>
                <oer:attribute description="Interface source code" name="InterfaceSourceCode"/>
                <oer:attribute description="Item identifier" name="InventoryItemId"/>
                <oer:attribute description="Item revision" name="ItemRevision"/>
                <oer:attribute description="Orchestration code" name="OrchestrationCode"/>
                <oer:attribute description="Order less flag" name="OrderLessFlag"/>
                <oer:attribute description="Unique identifier of the Plant (or the manufacturing organization)"
                     name="OrganizationId"/>
                <oer:attribute description="The tolerance type for over completions. Possible values are “PERCENT” or “QUANTITY”"
                     name="OvercomplToleranceType"/>
                <oer:attribute description="The percent or quantity that is allowed over the scheduled completion work order quantity"
                     name="OvercomplToleranceValue"/>
                <oer:attribute description="Scheduled completion date of the Work Order" name="PlannedCompletionDate"/>
                <oer:attribute description="Scheduled start date of the Work Order" name="PlannedStartDate"/>
                <oer:attribute description="Quantity of the work order that is planned for and started with"
                     name="PlannedStartQuantity"/>
                <oer:attribute description="Rejected quantity" name="RejectedQuantity"/>
                <oer:attribute description="Date and time on which work order is released" name="ReleasedDate"/>
                <oer:attribute description="Indicates what scheduling  method the work order import process needs to apply. For R11 it can be “No Scheduling” (scheduling is not triggered during import) or “Default Scheduler ” (scheduling is triggered during import)"
                     name="SchedulingMethod"/>
                <oer:attribute description="SCO supply order id" name="ScoSupplyOrderId"/>
                <oer:attribute description="Scrsapped quantities" name="ScrappedQuantity"/>
                <oer:attribute description="Serial tracking flag" name="SerialTrackingFlag"/>
                <oer:attribute description="Any header level references of the source system’s object"
                     name="SourceHeaderRef"/>
                <oer:attribute description="Any header level reference id of the source system’s object"
                     name="SourceHeaderRefId"/>
                <oer:attribute description="Any line level references of the source system’s object"
                     name="SourceLineRef"/>
                <oer:attribute description="Any line level reference id of the source system’s object"
                     name="SourceLineRefId"/>
                <oer:attribute description="Source system identifier" name="SourceSystemId"/>
                <oer:attribute description="Source system type" name="SourceSystemType"/>
                <oer:attribute description="Supply types look up (Default: Based on work definition). This determines how each of the operation items would be supplied to the work order"
                     name="SupplyType"/>
                <oer:attribute description="Product quantity that would be measured (Primary UOM)"
                     name="UomCode"/>
                <oer:attribute description="Work definition date (earlier known as As of Date) which will be used for explosion of work definition into the work order"
                     name="WorkDefinitionAsOfDate"/>
                <oer:attribute description="Unique identifier of the Work Definition" name="WorkDefinitionId"/>
                <oer:attribute description="Work definition version identifier" name="WorkDefinitionVersionId"/>
                <oer:attribute description="Unique identifier of the Work Method" name="WorkMethodId"/>
                <oer:attribute description="Description of the Work Order" name="WorkOrderDescription"/>
                <oer:attribute description="Unique identifier to a work order" name="WorkOrderId"/>
                <oer:attribute description="Unique identifier of the work order to which the record detail belongs to"
                     name="WorkOrderNumber"/>
                <oer:attribute description="Work order priority" name="WorkOrderPriority"/>
                <oer:attribute description="Unique identifir of the status of the Work Order" name="WorkOrderStatusId"/>
                <oer:attribute description="Status of the Work Order" name="WoStatusCode"/>
                <oer:attribute description="This represents the specific nature of work being performed such as Standard Production, Prototyping, or Repair. Costing derives work order accounts based on this attribute"
                     name="WorkOrderSubType"/>
                <oer:attribute description="Based on the Work Method, this attribute further classifies the Work Order to indicate whether it is meant for either Standard or Non Standard Production"
                     name="WorkOrderType"/>
                <oer:attribute description="Plant (Manufacturing Organization) where the work order is created"
                     name="OrganizationCode"/>
                <oer:attribute description="Unique identifier of the Plant (or the manufacturing organization)"
                     name="OrganizationId1"/>
                <oer:attribute description="The quantity that will be the nettable supply quantity considered by the planning system from the work order"
                     name="NettableSupplyQtyOverride"/>
                <oer:attribute description="Work order WIP quantity" name="WoWipQty"/>
                <oer:attribute description="Operations associated to a work order" name="WorkOrderOperation"/>
                <oer:attribute description="work order operation materials asscociated to a work order"
                     name="WorkOrderOperationMaterial"/>
                <oer:attribute description="All the work order attachments" name="WorkOrderAttachment"/>
            </oer:attributes>
        </xsd:documentation>
    </xsd:annotation>
    <xsd:import namespace="http://xmlns.oracle.com/adf/svc/types/" schemaLocation="BC4JService.xsd"/>
    <xsd:import namespace="commonj.sdo/java" schemaLocation="sdoJava.xsd"/>
